# ~/.config/xfce4/xfconf/xfce-perchannel-xml/xsettings.xml

Net/ThemeName "Arc-Dark"
Net/IconThemeName "Papirus-Dark"
# Net/DoubleClickTime
# Net/DoubleClickDistance
# Net/DndDragThreshold
Net/CursorBlink 1
# Net/CursorBlinkTime
# Net/SoundThemeName
Net/EnableEventSounds 1
Net/EnableInputFeedbackSounds 1

# gpick dont show text with this
# Xft/DPI 96

Xft/Antialias 1
Xft/Hinting 1
Xft/HintStyle "hintfull"
Xft/RGBA "rgb"

Gtk/CanChangeAccels 0
# Gtk/ColorPalette
Gtk/FontName "Fira Sans,  11"
Gtk/MonospaceFontName "Iosevka Fixed,  12"
# Gtk/IconSizes
# Gtk/KeyThemeName
Gtk/ToolbarStyle 3
# Gtk/ToolbarIconSize
Gtk/MenuImages 1
Gtk/ButtonImages 1
# Gtk/MenuBarAccel
Gtk/CursorThemeName "Breeze_Red"
Gtk/CursorThemeSize 24
Gtk/DecorationLayout "icon,menu:minimize,maximize,close"
# Gtk/DialogsUseHeader
# Gtk/TitlebarMiddleClick
Gtk/EnableAnimations 1
Gtk/PrimaryButtonWarpsSlider 0

# Gdk/WindowScalingFactor
